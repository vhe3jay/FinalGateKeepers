<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXPasswordField?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>

<StackPane fx:id="stackPane" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="jilgatekeeper.LoginFormController">
   <children>
      <AnchorPane id="AnchorPane" prefHeight="300.0" prefWidth="300.0" style="-fx-background-color: white;">
         <children>
            <JFXPasswordField fx:id="pwTextField" focusTraversable="false" labelFloat="true" layoutX="83.0" layoutY="213.0" promptText="Password" />
            <JFXTextField fx:id="userTextField" focusTraversable="false" labelFloat="true" layoutX="83.0" layoutY="171.0" promptText="Username" />
            <JFXButton fx:id="loginButton" defaultButton="true" layoutX="173.0" layoutY="262.0" onAction="#loginForm" prefHeight="25.0" prefWidth="90.0" style="-fx-border-color: black;" text="Log in">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor></JFXButton>
            <JFXButton fx:id="cancelButton" cancelButton="true" layoutX="38.0" layoutY="262.0" onAction="#cancelButton" prefHeight="25.0" prefWidth="90.0" style="-fx-border-color: black;" text="Cancel">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor></JFXButton>
            <ImageView fitHeight="159.0" fitWidth="300.0">
               <image>
                  <Image url="@Logo%20Portrait.jpg" />
               </image>
            </ImageView>
         </children>
      </AnchorPane>
   </children>
</StackPane>
