<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>

<HBox minHeight="600.0" minWidth="800.0" stylesheets="@bootstrap3.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="jilgatekeeper.ListofAttendiesController">
   <children>
      <BorderPane prefHeight="600.0" prefWidth="800.0" HBox.hgrow="ALWAYS">
         <top>
            <Pane prefHeight="60.0" prefWidth="685.0">
               <children>
                  <JFXComboBox fx:id="lgcombo" focusTraversable="false" labelFloat="true" layoutX="281.0" layoutY="15.0" prefHeight="25.0" prefWidth="149.0" promptText="Life Group" styleClass="combo-box">
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                  </JFXComboBox>
                  <JFXComboBox fx:id="sort_attendy" focusTraversable="false" labelFloat="true" layoutX="632.0" layoutY="14.0" prefHeight="25.0" prefWidth="151.0" promptText="Today">
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                  </JFXComboBox>
                  <Button layoutX="178.0" layoutY="14.0" mnemonicParsing="false" style="-fx-border-color: blue;" text="Clear Filters" />
                  <JFXTextField layoutX="26.0" layoutY="15.0" />
               </children>
            </Pane>
         </top>
         <center>
            <TableView fx:id="tb" editable="true" prefHeight="535.0" prefWidth="770.0">
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy></TableView>
         </center>
      </BorderPane>
   </children>
</HBox>
